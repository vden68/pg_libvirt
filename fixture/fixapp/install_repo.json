{
  "ubuntu":{
    "steps": [
      "sudo sh -c 'echo \"deb http://repo.postgrespro.ru/pgpro-9.6/ubuntu $(lsb_release -cs) main\" > /etc/apt/sources.list.d/postgrespro.list'",
      "wget --quiet -O - http://repo.postgrespro.ru/pgpro-9.6/keys/GPG-KEY-POSTGRESPRO | sudo apt-key -y add -",
      "sudo apt-get -y update",
      "sudo apt-get  install -y postgrespro*"
      ]
  }
}